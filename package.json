{
	"name": "md-push-blog",
	"displayName": "md_push_blog",
	"description": "push markdown file to blog",
	"version": "0.0.1",
	"publisher": "sherman",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"activationEvents": [
		"onLanguage:markdown"
	],
	"contributes": {
		"commands": [
			{
				"command": "push.blog",
				"title": "push to blog"
			},
			{
				"command": "push.pasteImageToLocal",
				"title": "paste clipboard Image To Local"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == markdown",
					"command": "push.blog",
					"group": "push-article"
				}
			],
			"editor/context": [
				{
					"when": "resourceLangId == markdown",
					"command": "push.blog",
					"group": "push-article"
				},
				{
					"when": "resourceLangId == markdown",
					"command": "push.pasteImageToLocal",
					"group": "push-article"
				}
			],
			"editor/title/context": [
				{
					"when": "resourceLangId == markdown",
					"command": "push.blog",
					"group": "push-article"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"build": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@types/markdown-it": "^12.0.1",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"markdown-it": "^12.0.4",
		"moment": "^2.29.1",
		"request": "^2.88.2",
		"xml2js": "^0.4.23"
	}
}
